{
    "name": "divspace/soberbrite",
    "type": "project",
    "description": "The source code for the Soberbrite community.",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/framework": "^7.24",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^2.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "rector/rector": "^0.7",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.9"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ecs": "ecs check --ansi --fix",
        "phpcs": "php-cs-fixer fix -v --allow-risky=yes --ansi --verbose",
        "rector": "rector process --ansi",
        "test": [
            "@test:ecs",
            "@test:phpcs",
            "@test:rector"
        ],
        "test:ecs": "ecs check --ansi",
        "test:phpcs": "php-cs-fixer fix --allow-risky=yes --ansi --dry-run --verbose",
        "test:rector": "rector process --ansi --dry-run"
    }
}
